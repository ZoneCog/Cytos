<root xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation='MSystemXMLSchema.xsd'>
  <tiling>
    <tiles>
      <tile name="d">
        <polygon>
          <sides value="4"/>
          <radius value="2"/>
        </polygon>
        <positions>
          <position name="center">
            <posX value="0"/>
            <posY value="0"/>
          </position>
        </positions>
        <connectingAngle value="90"/>
        <connectors>
          <connector name="c1">
            <positions>
              <position name="v1"/>
              <position name="v2"/>
            </positions>
            <glue name="g1"/>
          </connector>

          <connector name="c2">
            <positions>
              <position name="v2"/>
              <position name="v3"/>
            </positions>
            <glue name="g1"/>
          </connector>

          <connector name="c3">
            <positions>
              <position name="v3"/>
              <position name="v4"/>
            </positions>
            <glue name="g1"/>
          </connector>

          <connector name="c4">
            <positions>
              <position name="v4"/>
              <position name="v1"/>
            </positions>
            <glue name="g1"/>
          </connector>

        </connectors>
        <surfaceGlue name="gx"/>
        <color name="Blue" alpha="128"/>
      </tile>
    </tiles>
    <glues>
      <glue name="g1"/>
      <glue name="g2"/>
      <glue name="g3"/>
      <glue name="g4"/>
      <glue name="gx"/>
    </glues>
    <glueRelations>
      <glueTuple glue1="g1" glue2="g1"/>
    </glueRelations>
    <initialObjects>
      <initialObject name="d">
        <posX value="0"/>
        <posY value="0"/>
        <posZ value="0"/>

        <angleX value="0"/>
        <angleY value="0"/>
        <angleZ value="0"/>
      </initialObject>
    </initialObjects>
    <glueRadius value="0.1"/>
	<randomMovement value ="0.0"/>
  </tiling>
  <Msystem>
    <floatingObjects>
      <floatingObject name="a">
        <shape value="capsule"/>
        <size value="0.05"/>
        <mobility value="5"/>
        <concentration value="10"/>
      </floatingObject>
    </floatingObjects>
    <proteins />
    <proteinsOnTiles />
    <evoRulesWithPriority>
      <evoRule type="Create">
        <leftside value="a"/>
        <rightside value="d"/>
      </evoRule>
      <evoRule type="Divide">
        <leftside value="g1,a,g1"/>
        <rightside value="g1,g1"/>
      </evoRule>
    </evoRulesWithPriority>
    <signalObjects />
  </Msystem>
</root>